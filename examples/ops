void main() {
    int a = 2;
    int b = 2;

    int i = 0;
    while (i < 10) {
        i = add(i, 1);
    }

    if add(12, 12) == 24 && 1 {
        555;
    }

    if (a != b && a == 2) {
        a = 1;

        if (a == 1) {
            a = 5;
        }
    }

    a;
    b;

    return;
}

int add(int a, int b) {
    return a + b;
}
